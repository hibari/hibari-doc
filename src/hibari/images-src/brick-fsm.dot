digraph data_flow {
    {rank=same; start unknown};
    start [label="Start"];
    unknown [label="Unknown"];
    pre_init [label="Pre_init"];
    repairing [label="Repairing"];
    ok [label="OK"];
    disk_error [label="Disk_error"];

    start -> unknown;
    unknown -> pre_init;
    pre_init -> repairing;
    pre_init -> ok;
    pre_init -> unknown;
    repairing -> ok;
    repairing -> unknown;
    ok -> disk_error;
    ok -> unknown;
    disk_error -> pre_init;
    disk_error -> unknown;
};
